{
  "name": "sstepkiz-backends",
  "version": "1.0.0",
  "description": "Nest.js backends for the SSteP-KiZ project.",
  "author": "Jonas Primbs",
  "private": true,
  "scripts": {
    "nest": "nest",
    "build:aggregator": "nest build aggregator",
    "build:gamification": "nest build gamification",
    "build:signalling": "nest build signalling",
    "clean": "rm -rf dist",
    "start:aggregator": "env NODE_ENV=dev nest start aggregator --watch",
    "start:aggregator:prod": "node dist/apps/aggregator/main",
    "start:gamification": "env NODE_ENV=dev nest start gamification --watch",
    "start:gamification:prod": "node dist/apps/gamification/main",
    "start:signalling": "env NODE_ENV=dev nest start signalling --watch",
    "start:signalling:prod": "node dist/apps/signalling/main",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --fix",
    "lint:aggregator": "eslint \"apps/aggregator/**/*.ts\" --fix",
    "lint:gamification": "eslint \"apps/gamification/**/*.ts\" --fix",
    "lint:signalling": "eslint \"apps/signalling/**/*.ts\" --fix",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e": "jest --config ./apps/backends/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.4",
    "@nestjs/platform-socket.io": "^9.0.4",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^9.0.4",
    "@nestjsplus/redirect": "^1.0.0",
    "@sideway/formula": "^3.0.0",
    "@sideway/pinpoint": "^2.0.0",
    "bcijs": "^1.8.0",
    "canvas": "^2.9.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cubic-spline": "^3.0.3",
    "joi": "^17.6.0",
    "jose": "^4.8.3",
    "lib": "^4.3.3",
    "moment": "^2.29.4",
    "node-notifier": "^10.0.1",
    "open": "^8.4.0",
    "openid-client": "^5.1.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7",
    "usb-detection": "^4.14.1",
    "uuid": "^8.3.2",
    "wrtc": "^0.4.7",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.4",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "28.1.6",
    "@types/node": "^18.0.6",
    "@types/node-notifier": "^8.0.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ignore-loader": "^0.1.2",
    "jest": "28.1.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/",
      "<rootDir>../shared/"
    ],
    "moduleNameMapper": {
      "@libs/auth/(.*)": "<rootDir>/libs/auth/src/$1",
      "@libs/auth": "<rootDir>/libs/auth/src",
      "@libs/apdm/(.*)": "<rootDir>/libs/apdm/src/$1",
      "@libs/apdm": "<rootDir>/libs/apdm/src",
      "@libs/client-auth/(.*)": "<rootDir>/libs/client-auth/src/$1",
      "@libs/client-auth": "<rootDir>/libs/client-auth/src",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src",
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/db": "<rootDir>/libs/db/src",
      "@libs/hearty-patch/(.*)": "<rootDir>/libs/hearty-patch/src/$1",
      "@libs/hearty-patch": "<rootDir>/libs/hearty-patch/src",
      "@libs/logger/(.*)": "<rootDir>/libs/logger/src/$1",
      "@libs/logger": "<rootDir>/libs/logger/src",
      "@libs/look/(.*)": "<rootDir>/libs/look/src/$1",
      "@libs/look": "<rootDir>/libs/look/src",
      "@libs/movesense/(.*)": "<rootDir>/libs/movesense/src/$1",
      "@libs/movesense": "<rootDir>/libs/movesense/src",
      "@libs/rtc-sender/(.*)": "<rootDir>/libs/rtc-sender/src/$1",
      "@libs/rtc-sender": "<rootDir>/libs/rtc-sender/src",
      "@libs/udp-connector/(.*)": "<rootDir>/libs/udp-connector/src/$1",
      "@libs/udp-connector": "<rootDir>/libs/udp-connector/src",
      "@libs/usb-device/(.*)": "<rootDir>/libs/usb-device/src/$1",
      "@libs/usb-device": "<rootDir>/libs/usb-device/src",
      "@sstepkiz/(.*)": "<rootDir>/../shared/dist/$1",
      "@sstepkiz": "<rootDir>/../shared/dist"
    }
  }
}
