{
  "name": "sstepkiz-frontends",
  "version": "1.0.0",
  "description": "Angular frontend applications for the SSteP-KiZ project.",
  "author": "Jonas Primbs",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build:admin": "ng build --configuration production --project=admin-ui",
    "build:aggregator": "ng build --configuration production --project=aggregator-ui",
    "build:patient": "ng build --configuration production --project=patient-ui",
    "build:therapist": "ng build --configuration production --project=therapist-ui",
    "clean": "rm -rf dist && rm -rf .angular",
    "lint": "ng lint --fix",
    "postinstall": "ngcc --tsconfig \"tsconfig.json\"",
    "start:admin": "ng serve --project=admin-ui --port=4100",
    "start:aggregator": "ng serve --project=aggregator-ui --port=4000",
    "start:patient": "ng serve --project=patient-ui --port=4200",
    "start:therapist": "ng serve --project=therapist-ui --port=4300",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "dependencies": {
